@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":ExpenditureExpertParser" as ExpenditureExpertParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedExpenditureExpert" as VersionedExpenditureExpert MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> ExpenditureExpertParser : parseCommand(undo)
activate ExpenditureExpertParser

create UndoCommand
ExpenditureExpertParser -> UndoCommand
activate UndoCommand

UndoCommand --> ExpenditureExpertParser
deactivate UndoCommand

ExpenditureExpertParser --> LogicManager : u
deactivate ExpenditureExpertParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoExpenditureExpert()
activate Model

Model -> VersionedExpenditureExpert : undo()
activate VersionedExpenditureExpert

VersionedExpenditureExpert -> VersionedExpenditureExpert :resetData(ReadOnlyExpenditureExpert)
VersionedExpenditureExpert --> Model :
deactivate VersionedExpenditureExpert

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
